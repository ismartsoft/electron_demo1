{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "stopOnEntry": true,
            "sourceMaps": true,
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "runtimeArgs": [
                "--nolazy"
            ],
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/src/main.js",
            "protocol": "inspector",
            "restart": true
        }
    ]
}
